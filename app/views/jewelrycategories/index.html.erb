<% if can? :update, @jewelrycategory %>

<p id="notice"><%= notice %></p>

<h1>Listing Jewelrycategories</h1>

<table>
  <thead>
    <tr>
      <th>Jewelrycategory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jewelrycategories.each do |jewelrycategory| %>
      <tr>
        <td><%= jewelrycategory.jewelrycategory %></td>
        <td><%= link_to 'Show', jewelrycategory %></td>
        <td><%= link_to 'Edit', edit_jewelrycategory_path(jewelrycategory) %></td>
        <td><%= link_to 'Destroy', jewelrycategory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jewelrycategory', new_jewelrycategory_path %>

<% end %>